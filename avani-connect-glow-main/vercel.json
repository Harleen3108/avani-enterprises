{
  "functions": {
    "api/seo.js": {
      "includeFiles": "dist/template.html",
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/news-sitemap.xml",
      "destination": "/sitemap.xml",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/((?!api|assets|favicon.ico|logo.png|.*\\..*).*)",
      "destination": "/api/seo?path=/$1"
    }
  ]
}